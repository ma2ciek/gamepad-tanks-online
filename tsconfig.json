{
    "compilerOptions": {
        "lib": [
            "es5",
            "es6",
            "es7",
            "dom"
        ],
        "module": "commonjs",
        "target": "es2015",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,

        "moduleResolution": "node"
    }
}
